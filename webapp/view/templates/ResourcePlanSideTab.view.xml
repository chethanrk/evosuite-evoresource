<mvc:View height="100%" controllerName="com.evorait.evosuite.evoresource.controller.ResourcePlanSideTab" xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.f" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns:smartTable="sap.ui.comp.smarttable"
	displayBlock="true" xmlns="sap.m" xmlns:smartvariants="sap.ui.comp.smartvariants" xmlns:dnd="sap.ui.core.dnd"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1">
	<template:alias name=".AH" value="sap.ui.model.odata.AnnotationHelper">
		<template:alias name=".evoAH" value="com.evorait.evosuite.evoresource.model.AnnotationHelper">
			<template:with path="templateProperties>/" var="tempProps">
				<Page id="idResourceGroupPage" showHeader="false">
					<IconTabBar id="idIconTabBar" stretchContentHeight="true" class="sapUiNoContentPadding">
						<items>
							<template:with path="tempProps>/IconTabBarTable" helper="sap.ui.model.odata.AnnotationHelper.resolvePath" var="contentTabs">
								<template:repeat list="{contentTabs>}">
									<IconTabFilter id="idTab--{path:'contentTabs>tabName'}" text="{:= ${contentTabs>header}}" key="{:= ${contentTabs>tabName}}">
										<f:DynamicPage id="idDynamicPage--{path:'contentTabs>tabName'}" headerExpanded="true" fitContent="true" preserveHeaderStateOnScroll="true"
											toggleHeaderOnTitleClick="false" class="sapUiNoContentPadding">
											<f:header>
												<f:DynamicPageHeader id="idDynamicPageHeader--{path:'contentTabs>tabName'}" pinnable="true" class="sapUiNoContentPadding">
													<f:content>
														<smartFilterBar:SmartFilterBar id="idSmartFilter--{path:'contentTabs>tabName'}"
															persistencyKey="com.evorait.evosuite.evoresource.ResourceGroupFilterbar" entitySet="{:= ${contentTabs>entitySet}}" showClearButton="true"
															useToolbar="true" liveMode="true">
															<smartFilterBar:controlConfiguration>
																<core:ExtensionPoint name="FilterExtP|{path:'contentTabs>tabName'}|{path:'contentTabs>entitySet'}|ControlConfiguration"/>
															</smartFilterBar:controlConfiguration>
															<smartFilterBar:groupConfiguration>
																<core:ExtensionPoint name="FilterExtP|{path:'contentTabs>tabName'}|{path:'contentTabs>entitySet'}|GroupConfiguration"/>
															</smartFilterBar:groupConfiguration>
														</smartFilterBar:SmartFilterBar>
													</f:content>
												</f:DynamicPageHeader>
											</f:header>
											<f:content>
												<smartTable:SmartTable header="{:= ${contentTabs>header}}" id="idSmartTable--{path:'contentTabs>tabName'}"
													smartFilterId="idSmartFilter--{path:'contentTabs>tabName'}" entitySet="{:= ${contentTabs>entitySet}}" enableAutoBinding="true"
													tableType="ResponsiveTable" demandPopin="true" useTablePersonalisation="false" useExportToExcel="false"
													requestAtLeastFields="{:= ${contentTabs>requestAtLeastFields}}">
													<smartTable:customToolbar>
														<Toolbar>
															<ToolbarSpacer/>
															<core:ExtensionPoint name="TableExtP|{path:'contentTabs>tabName'}|{path:'contentTabs>entitySet'}|CustomToolbar"/>
														</Toolbar>
													</smartTable:customToolbar>
													<Table id="idCustomTable--{path:'contentTabs>tabName'}">
														<columns>
															<core:ExtensionPoint name="TableExtP|{path:'contentTabs>tabName'}|{path:'contentTabs>entitySet'}|CustomColumn"/>
															<template:if test="{:= (${contentTabs>tabName} === 'TimeAllocation')}">
																<Column mergeDuplicates="true">
																	<header>
																		<Text text="Type"/>
																	</header>
																</Column>
															</template:if>
														</columns>
														<items>
															<ColumnListItem>
																<cells>
																	<template:if test="{:= (${contentTabs>tabName} === 'TimeAllocation')}">
																		<Label text="{TimeAllocationType}" design="Bold"/>
																	</template:if>
																	<core:ExtensionPoint name="TableExtP|{path:'contentTabs>tabName'}|{path:'contentTabs>entitySet'}|CustomCell"/>
																</cells>
															</ColumnListItem>
														</items>
														<dragDropConfig>
															<dnd:DragInfo groupName="resourceGroup" sourceAggregation="items" dragStart="onResourceGroupDragStart" dragEnd="onResourceGroupDragEnd"/>
														</dragDropConfig>
													</Table>
												</smartTable:SmartTable>
											</f:content>
										</f:DynamicPage>
									</IconTabFilter>
								</template:repeat>
							</template:with>
						</items>
					</IconTabBar>
				</Page>
			</template:with>
		</template:alias>
	</template:alias>
</mvc:View>