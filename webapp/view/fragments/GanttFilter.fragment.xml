				<!--<CheckBox text="{i18n>xlbl.showShift}" selected="{user>/ENABLE_SHOW_SHIFT}" select="onGanttEnableType"/>-->
				<!--<CheckBox text="{i18n>xlbl.showGroup}" selected="{user>/ENABLE_SHOW_GROUP}" select="onGanttEnableType"/>-->
