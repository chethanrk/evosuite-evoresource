<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.f">
	<f:DynamicPageTitle>
		<f:heading>
			<Title text="{i18n>appTitle}"/>
			<!--<Image src="{path: '', formatter: '.formatter.getLogoImageLink' }" densityAware="false" height="30px" alt="EvoResource" id="imageLogo"/>-->
			<core:ExtensionPoint name="PageExtP|PageTitleAndActions|Title|Heading"/>
		</f:heading>
		<f:actions>
			<core:ExtensionPoint name="PageExtP|PageTitleAndActions|Title|Actions"/>
			<Button icon="sap-icon://alert" text="{= ${coreMessageModel>/}.length}" type="Emphasized" press="onMessageManagerPress"/>
			<Button id="idButtonAboutDialog" icon="sap-icon://sys-help" tooltip="{i18n>xtit.infoDialogTitle}" press="onAboutIconPress"/>
		</f:actions>
	</f:DynamicPageTitle>
</core:FragmentDefinition>