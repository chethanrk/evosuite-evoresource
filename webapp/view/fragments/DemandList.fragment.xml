<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form">
	<Dialog title="{i18n>xtit.demands}" resizable="true">
		<List items="{ path: 'demandModel>/data' }">
			<ObjectListItem title="{demandModel>DemandDesc}" type="Active" press="onListItemPress">
				<ObjectAttribute title="Date From" text="{path:'demandModel>DateFrom',type: 'sap.ui.model.type.Date', formatOptions: { style: 'short' }}"/>
				<ObjectAttribute title="Date To" text="{path:'demandModel>DateTo',type: 'sap.ui.model.type.Date', formatOptions: { style: 'short' }}"/>
			</ObjectListItem>
		</List>
	</Dialog>
</core:FragmentDefinition>