<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form">
	<Dialog title="{i18n>xtit.demands}" resizable="true" contentWidth="50%">
		<List items="{ path: 'demandModel>/data' }">
			<infoToolbar>
				<OverflowToolbar active="true">
					<Label text="Edit/Delete of selected assignment is not possible because these demands are assigned for this group"/>
				</OverflowToolbar>
			</infoToolbar>
			<ObjectListItem title="{demandModel>DemandDesc}">
				<firstStatus>
					<ObjectStatus text="{i18n>lbl.showDemand}" active="true" state="Information" press="onShowDemandPress"/>
				</firstStatus>
				<ObjectAttribute title="From Date" text="{path:'demandModel>DateFrom',type: 'sap.ui.model.type.Date', formatOptions: { style: 'short' }}"/>
				<ObjectAttribute title="To Date" text="{path:'demandModel>DateTo',type: 'sap.ui.model.type.Date', formatOptions: { style: 'short' }}"/>
			</ObjectListItem>
		</List>
		<endButton>
			<Button text="{i18n>btn.close}" press="onDemandDialogClose"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>