<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="{i18n>xtit.deletingAssignmentlist}" resizable="true" draggable="true" contentWidth="30%" contentHeight="70%"
		verticalScrolling="false" horizontalScrolling="false">
		<IconTabBar stretchContentHeight="true">
			<items>
				<core:Fragment fragmentName="com.evorait.evosuite.evoresource.view.fragments.iconTabFilters.DeletableAssignmentList" type="XML"/>
				<core:Fragment fragmentName="com.evorait.evosuite.evoresource.view.fragments.iconTabFilters.NonDeletableAssignmentList" type="XML"/>
				<core:Fragment fragmentName="com.evorait.evosuite.evoresource.view.fragments.iconTabFilters.UnassignmentDemandList" type="XML"/>
			</items>
		</IconTabBar>
		<beginButton>
			<Button text="{i18n>btn.continueDelete}" press="onDeleteAllAssignment" type="Emphasized"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>btn.cancel}" press="onDeleteAssignmentDialogClose" type="Emphasized"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>